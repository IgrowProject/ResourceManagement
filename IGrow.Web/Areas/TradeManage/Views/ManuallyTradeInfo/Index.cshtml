                { field: 'OD55', title: '新零售导购门店名称', width: 100, align: 'center' },
                { field: 'OD56', title: '新零售导购门店id', width: 100, align: 'center' },
                { field: 'OD57', title: '新零售发货门店名称', width: 100, align: 'center' },
                { field: 'OD58', title: '新零售发货门店id', width: 100, align: 'center' },
                { field: 'OD59', title: '退款金额', width: 100, align: 'center' },
                { field: 'OD60', title: '预约门店', width: 100, align: 'center' },
                { field: 'OD61', title: '是否村淘订单', width: 100, align: 'center' },
                { field: 'OD62', title: '确认收货时间', width: 100, align: 'center' },
                { field: 'OD63', title: '打款商家金额', width: 100, align: 'center' },
                { field: 'OD64', title: '含应开票给个人的个人红包', width: 100, align: 'center' },
                { field: 'OD65', title: '是否信用购', width: 100, align: 'center' },
                { field: 'OD66', title: '体验期结束时间', width: 100, align: 'center' },
                { field: 'OD67', title: '前N有礼', width: 100, align: 'center' },
                { field: 'OD68', title: '配送类型', width: 100, align: 'center' },
                { field: 'OD69', title: '主订单编号', width: 100, align: 'center' },
                { field: 'Description', title: '备注', width: 300, align: 'center' }
            ]]
        });

        $("#btn_search").click(function () {
            var queryJson = {
                ComCode: $("#txt_Code").val(),
                ComName: $('#txt_Name').val(),
                TstartTime: $("#TstartTime").val(),
                TendTime: $("#TendTime").val() 
            }
            $('#dg').datagrid('load', { queryJson: JSON.stringify(queryJson) });
        });
    }


    function btn_input() {
        $.modalOpen({
            id: "Form",
            title: "导入数据",
            url: "/Api/ExcelManage/Import?table=ManuallyTradeInfo",
            width: "700px",
            height: "520px",
            callBack: function (iframeId) {
                top.frames[iframeId].submitForm();
            }
        }); 
    }
</script>



<div class="gridPanel" style="height:100%;">
    <div id="toolbar" style="padding:2px 5px;">
        
        <div class="dcui-search-area">
            开始：
            <input id="TstartTime" class="easyui-datebox" labelPosition="top" style="width: 120px;padding-left:15px;">
            结束：
            <input id="TendTime" class="easyui-datebox" labelPosition="top" style="width: 120px;padding-left:15px;">
            订单编号:
            <input class="easyui-textbox" style="width:110px" id="txt_Code" placeholder="请输入要查询订单编号">
            支付单号：
            <input class="easyui-textbox" style="width:110px" id="txt_Name" placeholder="请输入要查询支付单号">
            <a href="#" class="easyui-linkbutton" iconCls="icon-search" id="btn_search">查询</a>
        </div>
    </div>

    <table id="dg" class="easyui-datagrid" style="width:100%;min-height:600px;height:100%;"></table>
</div>